// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
}

model User {
  id       String @id @unique
  name     String
  email    String @unique
  password String
  role     String
  token    String?
}

model Email {
  id       String @id @unique
  name     String
  from    String
  to String
  html     String
  sended_at DateTime
}