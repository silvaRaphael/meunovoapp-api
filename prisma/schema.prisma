generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
}

model User {
  id       String @id @unique
  name     String
  email    String @unique
  password String
  role     String
  token    String?
}

model Email {
  id       String @id @unique
  name     String
  from    String
  to String[]
  subject String
  html     String
  replyed String?
  has_reply Boolean?
  created_at DateTime
}