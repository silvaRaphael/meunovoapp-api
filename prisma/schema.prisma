generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("DATABASE_SHADOW_URL")
}

model User {
  id       String @id @unique
  name     String
  email    String @unique
  password String
  role     String
  token    String?

  @@map("users")
}
